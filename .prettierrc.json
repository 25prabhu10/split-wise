{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "tabWidth": 2,
  "useTabs": false,
  "endOfLine": "lf",
  "printWidth": 100,
  "singleQuote": true,
  "trailingComma": "none",
  "bracketSameLine": true,
  "tailwindStylesheet": "./src/styles/app.css",
  "tailwindFunctions": ["clsx"],
  "plugins": ["prettier-plugin-packagejson", "prettier-plugin-tailwindcss"]
}
